<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "item": {
          "@id": "{{ siteData.domain }}",
          "name": "{{ siteData.name }}"
        }
      },
      {
        "@type": "ListItem",
        "position": 2,
        "item": {
          "@id": "{{ siteData.domain }}{{ siteData.rootPath }}{{ pageData.path }}",
          "name": "{{ pageData.title }}"
        }
      }
    ]
  }
</script>
